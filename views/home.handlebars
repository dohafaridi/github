<nav class="navbar navbar-expand-lg navbar-dark bg-primary">
    <a class="navbar-brand" href="#">Github</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01"
        aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarColor01">
        <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
                <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="#">About</a>
            </li>
        </ul>
        <form method="post" action="/users" class="form-inline my-2 my-lg-0">
            <input name="user" class="form-control mr-sm-2" type="text" placeholder="User...">
            <button class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
        </form>
    </div>
</nav>
<main>
    <div class="container">
        <div class="row">
            {{#each repos}}
            <div class="card text-white bg-primary col-4 mt-5">
                <div class="card-header">{{this.full_name}}</div>
                <div class="card-body">
                    <h4 class="card-title">{{this.full_name}}</h4>
                    <p class="card-text">{{this.description}}</p>
                </div>
            </div>
            {{else}}
            <p class="empty">No content</p>
            {{/each}}
        </div>
</main>